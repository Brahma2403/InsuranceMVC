<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
</head>
<body>
    <body>
        <table width="100%" height="100%" border="1">
            <tr>
                <th width="100%">
                    @Html.Partial("Header")
                </th>
            </tr>
            <tr>
                <th width="100%">
                    @RenderBody()
                </th>
            </tr>
            <tr>
                <th width="100%">
                    @Html.Partial("Footer")
                </th>
            </tr>
        </table>
    </body>
</body>
</html>
